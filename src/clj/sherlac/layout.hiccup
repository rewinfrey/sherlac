(doctype :html5)
[:html
 [:head
  [:meta {:http-equiv "Content-Type" :content "text/html" :charset "iso-8859-1"}]
  [:title "sherlac"]
  (include-css (add-fingerprint "/stylesheets/sherlac.css"))
  (include-js (add-fingerprint (format "/javascript/sherlac%s.js" (if (env/development?) "_dev" ""))))]
 [:body
  (eval (:template-body joodo.views/*view-context*))
]]
